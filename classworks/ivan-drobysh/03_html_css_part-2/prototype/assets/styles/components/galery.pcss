.galery{
  width:90%;
  margin-left: 5%;
  display: flex;
  justify-content: space-between;

  &__column {
    display: flex;
    flex-direction: column;
    align-items: center;
    flex-basis: 33.3%;
  }

  &__element {
    margin:10px 20px;
    background-color: #FFFFFF;

  }

  &__text {
    font-size:17px;
    color: #888888;
    padding: 5px 0;
    text-align: center;
  }

  &__image {
  }
}
